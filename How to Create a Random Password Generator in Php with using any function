<?php
function rand_password($length){
	$str='asdasdadahadjdasggk234624235742@8213#$%&!@$&8123234ggdasdjk132313@wqeq';
	$len=strlen($str);
	$password='';
	for($i=0;$i<$length;$i++)
	{
		$random_index=rand(0,$len);
		$password.=$str[$random_index];
		
	}
	return $password;
}
$length=8;
echo rand_password($length,$c);
?>
